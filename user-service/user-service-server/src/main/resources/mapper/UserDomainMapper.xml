<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gitgle.mapper.UserDomainMapper">

    <resultMap id="BaseResultMap" type="com.gitgle.entity.UserDomain">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="login" column="login" jdbcType="VARCHAR"/>
            <result property="domainId" column="domain_id" jdbcType="VARCHAR"/>
            <result property="confidence" column="confidence" jdbcType="FLOAT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,login,domain_id,
        confidence
    </sql>
</mapper>
